<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/note/db" VERSION="20110225" COMMENT="XMLDB file for Moodle local/note"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="note" COMMENT="Default comment for local_note, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="title" TYPE="text" LENGTH="225" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="content" TYPE="text" LENGTH="1000" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="attachment" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="label" TYPE="text" LENGTH="1000" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="createdtime" TYPE="int" LENGTH="9" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="modifiedtime" TYPE="int" LENGTH="9" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="publisherid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="status" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
